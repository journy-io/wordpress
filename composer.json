{
  "name": "journy-io/wordpress",
  "description": "Official journy.io WordPress plugin",
  "type": "wordpress-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "journy.io",
      "email": "hi@journy.io"
    }
  ],
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5",
    "wp-coding-standards/wpcs": "^2.3",
    "szepeviktor/phpstan-wordpress": "^0.7.5",
    "phpstan/extension-installer": "^1.1"
  },
  "scripts": {
    "check-style": "phpcs src --ignore=*/views/*",
    "fix-style": "phpcbf src --ignore=*/views/*"
  },
  "autoload": {
    "psr-4": {
      "JournyIO\\WordPress\\": "./src"
    }
  },
  "require": {
    "journy-io/sdk": "^3.0",
    "kriswallsmith/buzz": "^1.2",
    "nyholm/psr7": "^1.4"
  }
}
